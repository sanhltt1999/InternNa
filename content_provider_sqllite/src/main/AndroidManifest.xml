<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="leeshani.com.content_provider_sqllite">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.InternNa">
        <activity
            android:name="leeshani.com.content_provider_sqllite.ui.editstudent.EditStudentActivity"
            android:exported="false" />
        <activity
            android:name="leeshani.com.content_provider_sqllite.ui.addstudent.AddStudentActivity"
            android:exported="false" />
        <activity
            android:name="leeshani.com.content_provider_sqllite.ui.addclass.AddClassActivity"
            android:exported="false" />
        <activity
            android:name="leeshani.com.content_provider_sqllite.ui.student.StudentsActivity"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <provider
            android:name=".SchoolContentProvider"
            android:authorities="com.example.content_provider_sqllite.provider"
            android:enabled="true"
            android:exported="true">
        </provider>

    </application>

</manifest>